{
    "builds": [
        {
            "src": "acu_masters//wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "headers": {
                "key": "Cache-Control",
                "value": "no-store"
            }
        },
        {
            "src": "/(.*)",
            "dest": "acu_masters//wsgi.py"
        }
    ]
}
